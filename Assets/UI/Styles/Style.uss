.buttonStyle {
    margin-left: 5%;
    margin-right: 5%;
    margin-top: 5%;
    margin-bottom: 5%;
    padding-left: 10%;
    padding-right: 10%;
    padding-top: 5%;
    padding-bottom: 5%;
    font-size: 100%;
    background-color: rgba(180, 180, 180, 0.71);
    border-top-left-radius: 4%;
    border-bottom-left-radius: 4%;
    border-top-right-radius: 4%;
    border-bottom-right-radius: 4%;
    border-left-width: 1px;
    border-right-width: 1px;
    border-top-width: 1px;
    border-bottom-width: 1px;
    border-left-color: rgb(0, 0, 0);
    border-right-color: rgb(0, 0, 0);
    border-top-color: rgb(0, 0, 0);
    border-bottom-color: rgb(0, 0, 0);
    width: auto;
    max-width: none;
    min-width: auto;
    min-height: auto;
    max-height: none;
    align-items: stretch;
    justify-content: flex-start;
    flex-direction: column;
    -unity-font: url('project://database/Assets/UI%20Toolkit/UnityThemes/UnityDefaultRuntimeTheme.tss?fileID=2230732570650464555&guid=5cdf01f97aa385d4e9d645e366f0d4ba&type=3#NotInter-Regular');
    -unity-font-style: normal;
}

